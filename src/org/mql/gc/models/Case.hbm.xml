<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
	<class name="org.mql.gc.models.Case" table="caseTab">
		<id name="id" column="id" type="int">
			<generator class="increment"/>
		</id>
		<property name="idAssociation" column="idAssociation" type="int"/>
		<property name="title" column="title" type="string"/>
		<property name="slogan" column="slogan" type="string"/>
		<property name="category" column="category" type="string"/>
		<property name="city" column="city"  type="string"/>
		<property name="endDate" column="endDate" type="date"/>
		<property name="cost" column="cost" type="double"/>
		<property name="description" column="description" type="string"/>
		<property name="addedDate" column="addedDate" type="timestamp"/>
		<property name="fileContent1" column="filecontent1" type="string"/>
		<property name="fileContent2" column="filecontent2" type="string"/>
		<property name="fileContent3" column="filecontent3" type="string"/>
		<property name="fileContentI" column="filecontentI" type="string"/>
		<property name="fileContentV" column="filecontentV" type="string"/>
		<property name="fileName1" column="fileName1" type="string"/>
		<property name="fileName2" column="fileName2" type="string"/>
		<property name="fileName3" column="fileName3" type="string"/>
	</class>
</hibernate-mapping>