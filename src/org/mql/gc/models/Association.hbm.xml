<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

<class name="org.mql.gc.models.Association" table="ASSOCIATION" >


	<id name="id" column="ID_ASSOCIATION" type="java.lang.Long" >
		<generator class="native"></generator>
	</id>
	<property name="name" column="NAME"></property>
	
	<property name="description" column="DESCRIPTION"></property>
	<property name="LowForm" column="LOWFORM" ></property>
	<property name="email" column="EMAIL" ></property>
	<property name="password" column="PASSWORD"></property>
	<property name="pattente" column="PATTENTE" ></property>
	<property name="activityArea" column="activityArea"></property>
	<property name="website" column="WEBSITE"></property>
	<property name="fixe" column="FIXE"></property>
	<property name="gsm" column="GSM"></property>
	
</class>


<!-- hbm donateur -->
<class name="org.mql.gc.models.Donnateur" table="DONNATEUR" >


	<id name="id" column="ID" type="int" >
		<generator class="native"></generator>
	</id>
	
	<property name="fName" column="FNAME"></property>
	
	<property name="lName" column="LNAME"></property>
	<property name="email" column="EMAIL" ></property>
	<property name="adresse" column="ADRESSE" ></property>
	<property name="ville" column="VILLE"></property>
	<property name="password" column="PASSWORD"></property>
	
	

</class>

 
  
</hibernate-mapping>